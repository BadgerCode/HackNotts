<UserControl x:Class="Timetabling_App.Pages.ModuleWizard.ModuleWizard"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:timetablingApp="clr-namespace:Timetabling_App"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}">
    
    <Grid x:Name="LayoutRoot" Background="Transparent">
        <timetablingApp:SelectCourse></timetablingApp:SelectCourse>
        <timetablingApp:SelectSchool></timetablingApp:SelectSchool>
    </Grid>
</UserControl>